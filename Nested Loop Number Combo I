#include <unistd.h>

void	ft_putchar(char c)
{
	write(1, &c, 1);
}

void	ft_putnumber(int i1, int i2, int i3)
{
	ft_putchar(i1 + '0');
	ft_putchar(i2 + '0');
	ft_putchar(i3 + '0');
	if (i1 != 7)
	{
		ft_putchar(',');
		ft_putchar(' ');
	}
}

void	ft_print_comb(void)
{
	int i;
	int j;
	int k;

	i = -1;
	j = 0;
	k = 1;
	while (i++ < j)
	{
		while (j++ < k)
		{
			while (k++ < 9)
			{
				ft_putnumber(i, j, k);
			}
			k = j + 1;
		}
		j = i + 1;
		k = j + 1;
	}
}

int	main(void)
{
	ft_print_comb();
	return (0);
}
